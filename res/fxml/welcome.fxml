<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.Glow?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="659.0" prefWidth="1129.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.welcome_controller">

    <children>
        <ImageView fitHeight="677.0" fitWidth="1128.0" pickOnBounds="true">
            <image>
                <Image url="@../BACKGROUND.jpeg" />
            </image>
         <effect>
            <Glow />
         </effect>
        </ImageView>
        <ImageView fx:id="new_game_image" accessibleRole="BUTTON" fitHeight="89.0" fitWidth="241.0" layoutX="448.0" layoutY="149.0" onMouseClicked="#clicked_new_game" onMouseEntered="#mouse_entered_new_game" onMouseExited="#mouse_exited_new_game" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../NEWGAME.png" />
            </image>
        </ImageView>
        <ImageView fx:id="load_game_image" accessibleRole="BUTTON" fitHeight="94.0" fitWidth="241.0" layoutX="450.0" layoutY="246.0" onMouseClicked="#clicked_load_game" onMouseEntered="#mouse_entered_load_game" onMouseExited="#mouse_exited_load_game" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../LOAD.png" />
            </image>
         <effect>
            <Glow />
         </effect>
        </ImageView>
        <ImageView fx:id="exit_image" accessibleRole="BUTTON" fitHeight="72.0" fitWidth="241.0" layoutX="450.0" layoutY="341.0" onMouseClicked="#clicked_exit_game" onMouseEntered="#mouse_entered_exit" onMouseExited="#mouse_exited_exit" pickOnBounds="true">
            <image>
                <Image url="@../EXIT.png" />
            </image>
         <effect>
            <Glow />
         </effect>
        </ImageView>
      <ImageView fx:id="choose_level_image" accessibleRole="BUTTON" fitHeight="72.0" fitWidth="315.0" layoutX="413.0" layoutY="427.0" onMouseClicked="#clicked_choose_level" onMouseEntered="#mouse_entered_choose_level" onMouseExited="#mouse_exited_choose_level" pickOnBounds="true">
         <image>
            <Image url="@../choose%20level.png" />
         </image>
         <effect>
            <Glow />
         </effect>
      </ImageView>
    </children>

</AnchorPane>
